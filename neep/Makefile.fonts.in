clean::
	$(RM) \
		@ISO8859_1_FONT@ @ISO8859_1_FONT_ALT@ \
		@ISO8859_2_FONT@ @ISO8859_2_FONT_ALT@ \
		@ISO8859_9_FONT@ @ISO8859_9_FONT_ALT@ \
		@ISO8859_15_FONT@ @ISO8859_15_FONT_ALT@ \
		@ASCII_FONT@ @ASCII_FONT_ALT@

