@ISO8859_1_FONT@: @ASCII_FONT@ @ISO8859_1_PARTS@
	$(CAT_FONTS) -o $@ \
		$(ADDSTYLE_REG) $(ENC_ISO8859_1) \
		@ASCII_FONT@ \
		@ISO8859_1_PARTS@

@ISO8859_1_FONT_ALT@: @ASCII_FONT_ALT@ @ISO8859_1_PARTS@
	$(CAT_FONTS) -o $@ \
		$(ADDSTYLE_ALT) $(ENC_ISO8859_1) \
		@ASCII_FONT_ALT@ \
		@ISO8859_1_PARTS@

