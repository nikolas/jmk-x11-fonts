@ISO8859_1_FONT@: @ASCII_FONT@ @ISO8859_1_PARTS@
	$(CAT_FONTS) -o $@ \
		$(ADDSTYLE_REG) $(ENC_ISO8859_1) \
		@ASCII_FONT@ \
		@ISO8859_1_PARTS@

@ISO8859_2_FONT@: @ASCII_FONT@ @ISO8859_2_PARTS@
	$(CAT_FONTS) -o $@ \
		$(ADDSTYLE_REG) $(ENC_ISO8859_2) \
		@ASCII_FONT@ \
		@ISO8859_2_PARTS@

clean::
	$(RM) \
		@ISO8859_1_FONT@ \
		@ISO8859_2_FONT@

